<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>SchulIT.IccImport</RootNamespace>
    <AssemblyName>SchulIT.IccImport</AssemblyName>
    <Authors>Marcel Marnitz</Authors>
    <Company>SchulIT</Company>
    <Copyright>SchulIT</Copyright>
    <PackageProjectUrl>https://github.com/SchulIT/iccimport-dotnet</PackageProjectUrl>
    <RepositoryUrl>https://github.com/SchulIT/iccimport-dotnet</RepositoryUrl>
    <AssemblyVersion>2.0.0.0</AssemblyVersion>
    <FileVersion>2.0.0.0</FileVersion>
    <Version>2.0.0.0</Version>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageTags>icc import .net core</PackageTags>
    <Description>This library provides functionalities to upload entities to the ICC.</Description>
    <PackageLicenseFile>LICENSE.md</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\LICENSE.md">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging" Version="3.1.14" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
